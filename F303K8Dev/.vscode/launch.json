{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Cortex Debug",
      "cwd": "${workspaceRoot}",
      "executable": "./build/F303K8Dev.elf",
      "request": "launch",
      "type": "cortex-debug",
      "servertype": "openocd",
      "device": "STM32F303K8",
      "configFiles": [
        "interface/stlink.cfg",
        "target/stm32f3x.cfg"
      ],
      "svdFile": "./STM32F303.svd",
      "liveWatch": {
        "enabled": true,
        "samplesPerSecond": 4
      },
      "overrideLaunchCommands": [
        "interpreter-exec console \"monitor reset halt\"",
        //'target-download',
        //'interpreter-exec console \"monitor reset halt\"',
        "-enable-pretty-printing"
      ],
    }
  ]
}